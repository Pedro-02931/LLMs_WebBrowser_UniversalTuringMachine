{
    "manifest_version": 3,
    "name": "SeuPainelDePrompts",
    "version": "1.0",
    "description": "Painel lateral com prompts predefinidos para LLMs.",
    "permissions": [
      "sidePanel",
      "activeTab"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "action": {
      "default_popup": "" 
    },
    "background": {
      "service_worker": "service-worker.js"
    }
    
  }